{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "solidity.enabledAsYouTypeCompilationErrorCheck": true,
  "cSpell.words": [
    "merkle",
    "sourcify",
    "typechain",
    "uups"
  ],
  "solidity.compileUsingRemoteVersion": "v0.8.4+commit.c7e474f2",
  "solidity.defaultCompiler": "remote",
}
