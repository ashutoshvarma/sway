{
  "rewrites": [
    {
      "source": "/metadata/:eventId/:tokenId",
      "destination": "/metadata/:eventId.json"
    },
    {"source": "/metadata/:eventId", "destination": "/metadata/:eventId.json"},
    {"source": "/badges/:eventId", "destination": "/badges/:eventId.png"},
    {"source": "/details/:eventId", "destination": "/details/:eventId.png"},
    {"source": "/merkle/:eventId", "destination": "/merkle/:eventId.png"},
    {"source": "/(.*)", "destination": "/404.json"}
  ]
}
