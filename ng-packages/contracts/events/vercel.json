{
  "rewrites": [
    { "source": "/metadata/:eventId/:tokenId", "destination": "/metadata/:eventId.json" },
    { "source": "/metadata/1/1", "destination": "/metadata/1.json" },
    { "source": "/proxy/:match*", "destination": "https://example.com/:match*" },
    {
      "source": "/:path((?!uk/).*)",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "GB"
        }
      ],
      "destination": "/uk/:path*"
    }
  ]
}
